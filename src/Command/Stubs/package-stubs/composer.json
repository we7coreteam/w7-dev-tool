{
	"name": "{{ name }}",
	"description": "A Rangine vendor.",
	"keywords": [
		"rangine"
	],
	"license": "Apache-2.0",
	"require": {
		"php": ">=7.1.0"
	},
	"autoload": {
		"psr-4": {
			"{{ escapedNamespace }}\\": "src/"
		}
	},
	"extra": {
		"rangine": {
			"providers": [
				"{{ escapedNamespace }}\\ServiceProvider"
			]
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
